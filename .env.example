# Copy to .env.local and fill in values. See README for descriptions.

# Gemini (required for audit)
GEMINI_API_KEY=

# Supabase (required)
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
NEXT_PRIVATE_SERVICE_ROLE_API_KEY=

# Discord webhook (required for approval flow)
DISCORD_WEBHOOK_URL=

# MailerLite (optional; used after approval)
MAILERLITE_API_KEY=
MAILERLITE_GROUP_ID=

# Floating contact widget (optional; FAB always visible, links need these)
# NEXT_PUBLIC_CONTACT_WHATSAPP=1234567890
# NEXT_PUBLIC_CONTACT_EMAIL=hello@example.com

# Optional
# GEMINI_MODEL=gemini-2.5-flash
# GEMINI_FALLBACK_MODEL=gemini-1.5-flash
